      "source": [
        "import numpy as np\n",
        "\n",
        "# x 값 의 수\n",
        "X = np.array([1, 2])\n",
        "print(f'x matrix: {X}')\n",
        "\n",
        "# 가중치 정렬(x1 가중치 1행, x2 가중치 2행...)\n",
        "W = np.array([[1, 3, 5], [2, 4, 6]])\n",
        "print(f'W matrix: {W}')\n",
        "\n",
        "# X * W(행렬 곱)\n",
        "Y = np.dot(X, W)\n",
        "print(f'Y matrix: {Y}')\n"
      ],
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": "x matrix: [1 2]\nW matrix: [[1 3 5]\n [2 4 6]]\nY matrix: [ 5 11 17]\n"
